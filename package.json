{
  "name": "mi-chatgpt",
  "version": "1.0.0",
  "description": "ChatGPT personalizado con backend en Node.js y TypeScript",
  "main": "backend/server.ts",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "nodemon --watch backend --ext ts --exec ts-node backend/server.ts",
    "build": "tsc",
    "prepare": "husky",
    "lint": "eslint backend/**/*.ts",
    "lint:fix": "eslint backend/**/*.ts --fix",
    "format": "prettier --write backend/**/*.ts",
    "format:check": "prettier --check backend/**/*.ts",
    "type-check": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "test": "echo \"No tests specified yet\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@mistralai/mistralai": "^1.7.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.30",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "backend/**/*.{ts,js}": [
      "eslint --fix --max-warnings 10",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
